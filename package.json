    "test": "npm run build && node --test"
  },
  "devDependencies": {
    "esbuild": "^0.23.1",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module"
}
